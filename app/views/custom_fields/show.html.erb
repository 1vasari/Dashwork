<% content_for(:title, @field.name) %>
<% content_for(:back_link, polymorphic_path([:custom, @domain.to_sym, :fields])) %>
<% content_for(:actions) do %>
  <%= render EditActionComponent.new path: polymorphic_path([:edit, @field]) %>
  <%= render DeleteActionComponent.new path: polymorphic_path([@field]), model_name: 'custom field' %>
<% end %>
