<%= simple_form_for company do |f| %>
  <%= render 'common/form_errors', form: f %>

  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :website %>
  <%= f.input :email %>
  <%= f.input :phone %>
  <%= f.input :address %>
  <%= f.association :tags, collection: current_tenant.company_tags.all %>

  <%= render 'common/custom_values_form', form: f, item: company %>

  <%= f.button :submit %>
<% end %>
