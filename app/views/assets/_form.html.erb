<%= simple_form_for(asset) do |f| %>
  <%= render 'common/form_errors', form: f %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :manufacturer %>
  <%= f.input :model %>
  <%= f.input :purchase_date %>
  <%= f.input :purchase_price %>
  <%= f.input :warranty_date %>
  <%= f.button :submit %>
<% end %>
