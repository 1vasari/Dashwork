# Personal Log Entries

<% current_day = nil %>
<% @entries.each do |entry| %><%# TODO: use find_each and retain ordering! %>
  <% if entry.entry_date != current_day %>

## <%= entry.decorate :day %>

    <% current_day = entry.entry_date %>
  <% end %>
**<%= entry.decorate :time %>**

<%= raw(entry.content) %>

<% end %>
